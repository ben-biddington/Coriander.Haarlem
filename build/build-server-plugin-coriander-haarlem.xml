<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-autowire="constructor">
     <bean id="DilbertController" class="coriander.haarlem.controllers.DilbertController" init-method="register" />
     <bean id="BeanListController" class="coriander.haarlem.controllers.BeanListController" init-method="register" />
     <bean id="dilbertTab" class="coriander.haarlem.tabs.DilbertTab" init-method="register">
      <property name="placeId" value="ALL_PAGES_HEADER "/>
      <property name="pluginName" value="dilbertTab"/>
      <property name="includeUrl" value="/dilbert.html"/>
    </bean>
</beans>
        